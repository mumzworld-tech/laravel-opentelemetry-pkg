{
    "name": "mumzworld/laravel-opentelemetry",
    "description": "OpenTelemetry integration package for Laravel applications with complete observability stack",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mumzworld Development Team",
            "email": "dev@mumzworld.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "open-telemetry/api": "^1.0",
        "open-telemetry/sdk": "^1.0",
        "open-telemetry/exporter-otlp": "^1.0",
        "open-telemetry/opentelemetry-auto-laravel": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mumzworld\\LaravelOpenTelemetry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mumzworld\\LaravelOpenTelemetry\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mumzworld\\LaravelOpenTelemetry\\LaravelOpenTelemetryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}